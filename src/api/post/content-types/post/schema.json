{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "Components": {
      "type": "dynamiczone",
      "components": [
        "my-components.code",
        "my-components.markdown",
        "my-components.video",
        "my-components.image"
      ]
    },
    "description": {
      "type": "text"
    },
    "featuredImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "posts"
    },
    "post_group": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::post-group.post-group",
      "inversedBy": "posts_in_group"
    }
  }
}
